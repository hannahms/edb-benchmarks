aws:
  cluster_name: DBT2-RDS
  ssh_user: rocky
  operating_system:
    name: Rocky-8-ec2-8.6-20220515.0.x86_64
    owner: 679593333241
  regions:
    {{ region }}:
      cidr_block: 10.0.0.0/16
      zones:
        {{ zone0 }}: 10.0.0.0/24
        {{ zone1 }}: 10.0.1.0/24
      service_ports:
        - port: 22
          protocol: tcp
          description: "SSH"
        - port: 30000
          protocol: tcp
          description: "DBT-2"
        - port: 30000
          protocol: udp
          description: "DBT-2"
        - port: 5432
          protocol: tcp
          description: "PostgreSQL"

  machines:
    dbt2-driver:
      type: dbt2-driver
      region: {{ region }}
      zone: {{ zone0 }}
      instance_type: {{ instance_type }}
      volume:
        type: gp2
        size_gb: 50
        iops: 5000
        encrypted: false

  databases:
    dbt2:
      region: {{ region }}
      engine: {{ pg_engine }}
      engine_version: {{ pg_version }}
      instance_type: {{ pg_instance_type }}
      dbname: "dbt2"
      username: {{ pg_superuser }}
      password: {{ pg_password }}
      port: {{ pg_port }}
      volume:
        size_gb: {{ pg_storage_size }}
        type: {{ pg_storage_type }}
        iops: {{ pg_storage_iops }}
        encrypted: true
